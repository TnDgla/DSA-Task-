class Solution {
    public int getCommon(int[] nums1, int[] nums2) {
        int i=0;
        int j=0;
        //int count=0;
        while(i<nums1.length && j<nums2.length){

            if(nums1[i]==nums2[j]){

                return nums2[j];
            }
            if(nums1[i]>nums2[j]){

                j++;
            }
            else{
                i++;
            }

        }
        return -1;
    }
}
