class Solution {
    public int maximumUniqueSubarray(int[] nums) {
        Map<Integer, Integer> map = new HashMap<>();
        int n = nums.length;
        int[] sum = new int[n + 1];
        int max = 0;
        int i = 0;
        for (int j = 0; j < n; j++) {
            sum[j + 1] = sum[j] + nums[j];
            if(map.containsKey(nums[j])){ 
                i = Math.max(i, map.get(nums[j]) + 1);
            }
            max = Math.max(max, sum[j + 1] - sum[i]);
            map.put(nums[j], j);
        }
        return max;
    }
}
