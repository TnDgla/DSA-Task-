class Solution {
    public int maxFrequencyElements(int[] nums) {
        int max = 0;
        Map<Integer, Integer> map = new HashMap<>();
        for(int i : nums){
            map.put(i, map.getOrDefault(i, 0) + 1);
            max = Math.max(max, map.get(i));
        }
        System.out.println(map);
        int count = 0;
        for(int i : map.keySet()){
            if(max == map.get(i)){
                count += map.get(i);
            }
        }
        return count;
    }
}
