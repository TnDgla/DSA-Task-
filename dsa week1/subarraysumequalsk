class Solution {
    public int subarraySum(int[] nums, int k) {
        int count=0;
        int sum=0;
        HashMap<Integer,Integer> min=new HashMap<>();
        min.put(0,1);
        for(int i=0;i<nums.length;i++){
            sum+=nums[i];
            if(min.containsKey(sum-k)){
                count+=min.get(sum-k);
            }
            min.put(sum,min.getOrDefault(sum,0)+1); 
        }
        return count;

        
    }
}