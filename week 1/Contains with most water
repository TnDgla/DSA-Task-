class Solution {
    public int maxArea(int[] height) {
        int i = 0;
        int j = height.length -1;
        int maxarea = 0,min = 0;
        while (i<j){
            min = Math.min(height[i], height[j]);
            maxarea = Math.max(maxarea, min*(j-i));
            while (min >= height[i] && i<j)
                i++;
            while(min >= height[j] && i<j)
                j--;
        }
        return maxarea;
    }
}